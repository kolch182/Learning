package ch.gmtech.ste.learning.html;
import static j2html.TagCreator.*;

public class Form{

	Form() {
		
	html().with(
		    head().with(
		        title("Course Form"),
		        meta().attr(charset, "utf8"),
		        link().withRel("stylesheet").withHref("/css/bootstrap.min.css")
		    ),
		    body().with(
		        main().with(
		            h1("Heading!")
		        )
		    )
		);
	}
	
}
