package ch.gmtech.ste.learning.html;
import static j2html.TagCreator.*;

public class Form{

	Form() {
		
	html().with(
		    head().with(
		        title("Title"),
		        link().withRel("stylesheet").withHref("/css/main.css")
		    ),
		    body().with(
		        main().with(
		            h1("Heading!")
		        )
		    )
		)
	}
	
}
